<Window x:Class="FileManager.View.CreateDirectory"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:FileManager.View"
        mc:Ignorable="d"
        Title="CreateDirectory" Height="150" Width="372.302">
    <Grid>
        <Button x:Name="okButton" Content="OK" HorizontalAlignment="Left" Margin="182,84,0,0" VerticalAlignment="Top" Width="75" Click="okButton_Click"/>
        <Button x:Name="cancelButton" Content="Cancel" HorizontalAlignment="Left" Margin="279,84,0,0" VerticalAlignment="Top" Width="75" Click="cancelButton_Click"/>
        <TextBox x:Name="textBox" HorizontalAlignment="Left" Height="23" Margin="10,33,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="344" TextChanged="textBox_TextChanged"/>
        <Label x:Name="label" Content="New directory name" HorizontalAlignment="Left" Margin="10,10,0,0" VerticalAlignment="Top" Width="232"/>
    </Grid>
</Window>
